{
  "extends": [
    "next/core-web-vitals", 
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "rules": {
    "import/order": ["error", { "newlines-between": "always" }],
    "import/export": "off",
    "indent": ["error", 4],
    "quotes": "error",
    "semi": "error",
    "max-len": ["error", { "code": 120 }],
    "comma-dangle": "error",
    "no-trailing-spaces": "error"
  }
}
